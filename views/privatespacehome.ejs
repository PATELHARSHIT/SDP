<%- include("partials/header") -%>
  <main role="main" class="container">
    <div class="row">
      <div class="col-md-9">
        <!-- Alert -->
        <%- include("partials/toast") %>
          <!-- Content -->
          <h1 class="article-title">

          </h1>

          <% questions.forEach(question=> { %>
            <article class="media">
              <div class="media-body content-section-private">
                <h5>
                  <a class="article-title" href="/privatespace/question/<%=question._id %>">
                    <%- question.ques %>
                  </a>
                </h5>
              </div>
            </article>

            <% }) %>
      </div>
      <div class="col-md-3 text-center">
        <br>
        <h5>Ask Your Query Privately.</h5>
        <br>
        <a href="/privatespace/privatequestion" class="btn btn-dark"><i class="fas fa-plus"></i> <b>Add One</b></a>
      </div>
    </div>

  </main>
  <%- include("partials/footer") -%>
