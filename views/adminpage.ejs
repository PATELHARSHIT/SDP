<%- include("partials/header") %>

    <main role="main">
        <div class="container">

            <div>
                <form action="">
                    <div class="form-row">
                        <div class="col">
                            <input class="form-control" type="text" name="" id="" placeholder="Category Name">
                        </div>
                        <div class="col">
                            <button class="btn btn-info">+ Add Category</button>
                        </div>
                    </div>

                </form>
            </div>
            <br>
            <div>
                <div class="row">

                    <% cat.forEach(c => { %>
                        <div class="col-md-3">
                            <p>
                                <%= c %>
                            </p>
                        </div>
                        <% }); %>
                </div>
            </div>
        </div>



        </div>
    </main>

    <%- include("partials/footer") %>